{
  "name": "@ecosystem/sage-stocks",
  "version": "0.1.0",
  "private": true,
  "description": "Sage Stocks v1.0 - Vercel Serverless + Notion Integration",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "test": "jest",
    "test:analyze": "ts-node scripts/test/analyze.ts",
    "test:notion-write": "ts-node scripts/test/notion-write.ts",
    "test:orchestrator": "ORCHESTRATOR_DRY_RUN=true ts-node scripts/test/orchestrator.ts",
    "test:orchestrator:real": "ORCHESTRATOR_DRY_RUN=false ts-node scripts/test/orchestrator.ts",
    "poll": "ts-node scripts/maintenance/poll-notion.ts",
    "cleanup-template": "ts-node scripts/maintenance/cleanup-prod-template.ts",
    "clear-tokens": "ts-node scripts/maintenance/clear-encrypted-tokens.ts",
    "clear-my-token": "ts-node scripts/maintenance/clear-my-token.ts",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "stock",
    "analysis",
    "notion",
    "vercel",
    "serverless"
  ],
  "author": "Shalom Ormsby",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@notionhq/client": "^5.4.0",
    "@vercel/edge-config": "^1.0.1",
    "@vercel/kv": "^1.0.1",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "openai": "^6.7.0",
    "vercel": "^48.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vercel/node": "^5.5.14",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.x"
  }
}
