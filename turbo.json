{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "env": ["STUDIO_URL"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {}
  }
}